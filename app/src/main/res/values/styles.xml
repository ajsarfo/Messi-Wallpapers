<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomToolbarStyle" parent="TextAppearance.MaterialComponents.Headline6">
        <item name="android:textColor">@color/toolbar_text</item>
        <item name="fontFamily">@font/primary_font</item>
    </style>

    <style name="CustomTabStyle" parent="Widget.AppCompat.ActionBar.TabText">
        <item name="android:textSize">14sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/toolbar_text</item>
    </style>

    <style name="CustomNavDrawerItemTextAppearance" parent="Widget.Design.NavigationView">
        <item name="fontFamily">@font/primary_font</item>
    </style>

    <style name="ThemeOverlay.ItemGroup" parent="">
        <item name="android:textColorSecondary">#AEAEAE</item>
    </style>
</resources>